
#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;


void main()
{ 
	int numero1,numero2,numero3; //declarar variables
		cout<<"ingrese el numero 1: "<<endl;              
	    cin>>numero1;
		cout<<"ingrese el numero 2: "<<endl;
		cin>>numero2;
		cout<<"ingrese el numero 3: "<<endl;
		cin>>numero3;
		if (numero1>=numero2)
			if (numero1>=numero3)
				cout<<"el mayor es: "<<numero1<<endl;
			else
				cout<<"el mayor es: " <<numero2<<endl;
		else
			if (numero2>=numero3)
				cout<<"el mayor es: "<<numero2<<endl;
			else
				cout<<"el mayor es: "<<numero3<<endl;
		getch();

}
